  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.25.0",
    "tsup": "^8.3.5",
    "typescript": "~5.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
