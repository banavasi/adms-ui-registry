{
  "name": "@banavasi/eslint-config",
  "version": "0.1.0",
  "description": "ESLint config for ADMS RDS UI projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
