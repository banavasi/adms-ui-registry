{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "declaration": true,
      "declarationMap": true,
      "isolatedModules": true,
      "verbatimModuleSyntax": true,
      "lib": ["ES2020", "DOM", "DOM.Iterable"]
    }
  }
